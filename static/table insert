DROP DATABASE IF EXISTS webprogProjekt;
CREATE DATABASE webprogProjekt;
USE webprogProjekt;

CREATE TABLE termekek(
    id INT PRIMARY KEY NOT NULL AUTO_INCREMENT,
    nev VARCHAR(45),
    tipus VARCHAR(45),
    ar DOUBLE,
    darab INT,
    ertekeles INT,
    kep VARCHAR(45) 
);

